@using CO.CDP.RegisterOfCommercialTools.App.Pages.Search
@model CO.CDP.RegisterOfCommercialTools.App.Pages.Search.SearchResultStatus

@switch (Model)
{
    case SearchResultStatus.Active:
        <strong class="govuk-tag govuk-tag--turquoise">Active</strong>
        break;
    case SearchResultStatus.Upcoming:
        <strong class="govuk-tag govuk-tag--yellow">Upcoming</strong>
        break;
    case SearchResultStatus.Expired:
        <strong class="govuk-tag govuk-tag--red">Expired</strong>
        break;
}

