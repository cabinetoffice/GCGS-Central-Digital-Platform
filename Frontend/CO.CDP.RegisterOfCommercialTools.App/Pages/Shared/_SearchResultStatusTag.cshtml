@using CO.CDP.RegisterOfCommercialTools.WebApiClient.Models
@model CO.CDP.RegisterOfCommercialTools.WebApiClient.Models.CommercialToolStatus

@switch (Model)
{
    case CommercialToolStatus.Active:
        <strong class="govuk-tag govuk-tag--turquoise">Active</strong>
        break;
    case CommercialToolStatus.Upcoming:
        <strong class="govuk-tag govuk-tag--yellow">Upcoming</strong>
        break;
    case CommercialToolStatus.Cancelled:
        <strong class="govuk-tag govuk-tag--orange">Cancelled</strong>
        break;
    case CommercialToolStatus.Awarded:
        <strong class="govuk-tag govuk-tag--blue">Awarded</strong>
        break;
    case CommercialToolStatus.Complete:
        <strong class="govuk-tag govuk-tag--purple">Complete</strong>
        break;
    case CommercialToolStatus.Unknown:
    default:
        <strong class="govuk-tag govuk-tag--grey">Unknown</strong>
        break;
}

