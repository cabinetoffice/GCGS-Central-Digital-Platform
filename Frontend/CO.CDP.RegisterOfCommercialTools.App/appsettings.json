{
  "Version": "dev",
  "Logging": {
    "Console": false,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "CommercialTools": false,
    "SwaggerUI": false,
    "UseOneLogin": true
  },
  "ForwardedHeaders": {
    "KnownNetworks": []
  },
  "FtsService": {
    "ServiceBaseUrl": "http://localhost:3000"
  },
  "SirsiService": {
    "ServiceBaseUrl": "http://localhost:8090"
  },
  "CommercialToolsApi": {
    "ServiceUrl": "http://localhost:8184"
  },
  "OneLogin": {
    "Authority": "",
    "ClientId": "",
    "PrivateKey": ""
  },
  "Organisation": {
    "Authority": "",
    "ClientSecret": ""
  },
  "Aws": {
    "SystemManager": {
      "DataProtectionPrefix": "/CommercialToolsApp/DataProtection"
    },
    "CloudWatch": {
      "LogGroup": "localstack",
      "LogStream": "serilog"
    },
    "ElastiCache": {
      "Hostname": "localhost",
      "Port": "6379"
    },
    "Region": "",
    "Credentials": {
      "AccessKey": "",
      "SecretKey": ""
    },
    "CognitoAuthentication": {
      "Domain": "",
      "UserPoolId": "",
      "UserPoolClientId": "",
      "UserPoolClientSecret": ""
    }
  },
  "SessionTimeoutInMinutes": 30
}
