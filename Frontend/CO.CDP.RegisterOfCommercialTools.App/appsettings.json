{
  "Version": "dev",
  "Logging": {
    "Console": false,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "CommercialTools": false,
    "ContentSecurityPolicy": true,
    "SwaggerUI": false,
    "UseOneLogin": true,
    "UseCognito": false,
    "SharedSessions": true,
    "DiagnosticPage": {
      "Enabled": false,
      "Path": ""
    }
  },
  "ForwardedHeaders": {
    "KnownNetwork": ""
  },
  "CommercialToolsApi": {
    "ServiceUrl": "",
    "ApiKey": "test-api-key-for-local-development"
  },
  "SirsiService": {
    "ServiceBaseUrl": ""
  },
  "FtsService": {
    "ServiceBaseUrl": ""
  },
  "Aws": {
    "Region": "eu-west-2",
    "SystemManager": {
      "DataProtectionPrefix": "/CDP/DataProtection"
    },
    "CloudWatch": {
      "LogGroup": "/ecs/commercial-tools-app",
      "LogStream": "serilog"
    },
    "ElastiCache": {
      "Hostname": "localhost",
      "Port": "6379"
    }
  },
  "SessionTimeoutInMinutes": 60,
  "OneLogin": {
    "Authority": "",
    "ClientId": "",
    "PrivateKey": "",
    "AccountUrl": "",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc"
  }
}
