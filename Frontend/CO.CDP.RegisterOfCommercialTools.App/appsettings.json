{
  "Version": "dev",
  "Logging": {
    "Console": false,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "CommercialTools": false,
    "SwaggerUI": false,
    "UseOneLogin": true,
    "UseCognito": false,
    "SharedSessions": true
  },
  "ForwardedHeaders": {
    "KnownNetworks": []
  },
  "CommercialToolsApi": {
    "ServiceUrl": ""
  },
  "SirsiService": {
    "ServiceBaseUrl": ""
  },
  "FtsService": {
    "ServiceBaseUrl": "",
    "ServiceUrl": ""
  },
  "Organisation": {
    "Authority": ""
  },
  "Aws": {
    "SystemManager": {
      "DataProtectionPrefix": "/CDP/DataProtection"
    },
    "CloudWatch": {
      "LogGroup": "/ecs/commercial-tools-app",
      "LogStream": "serilog"
    },
    "ElastiCache": {
      "Hostname": "localhost",
      "Port": "6379"
    },
    "Region": "placeholder",
    "Credentials": {
      "AccessKey": "placeholder",
      "SecretKey": "placeholder"
    },
    "CognitoAuthentication": {
      "Domain": "placeholder.auth.eu-west-2.amazoncognito.com",
      "UserPoolId": "placeholder",
      "UserPoolClientId": "placeholder",
      "UserPoolClientSecret": "placeholder"
    }
  },
  "SessionTimeoutInMinutes": 30,
  "OneLogin": {
    "Authority": "",
    "ClientId": "",
    "PrivateKey": "",
    "AccountUrl": "",
    "ForwardLogoutNotificationUrls": "",
    "ForwardLogoutNotificationApiKey": ""
  }
}
