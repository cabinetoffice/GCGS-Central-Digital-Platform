@page "/registration/organisation-already-registered/{Identifier}"

@model OrganisationAlreadyRegistered
@using CO.CDP.Localization

@{
    ViewData["Title"] = StaticTextResource.OrganisationRegistration_OrganisationAlreadyRegistered_Heading;
    var backLink = RegistrationStepModel.OrganisationIdentifierPage;
    var overrideBackLink = Request.Query.ContainsKey("backLink");

    if (overrideBackLink)
    {
        backLink = Request.Query["backLink"];
    }
}

<a href="@backLink" class="govuk-back-link">
    @StaticTextResource.Global_Back
</a>

<main class="govuk-main-wrapper" id="main-content">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-l">
                @ViewData["Title"]
            </h1>
            @Html.Raw(string.Format(StaticTextResource.OrganisationRegistration_OrganisationAlreadyRegistered_Message, Model.Identifier))
        </div>
    </div>
</main>