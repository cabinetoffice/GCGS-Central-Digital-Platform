@page "/privacy-notice"
@model PrivacyNoticeModel

@using CO.CDP.Localization
@using Microsoft.AspNetCore.Mvc.Localization

@{
    ViewData["Title"] = StaticTextResource.Supplementary_PrivacyNotice_Title;
}

<main class="govuk-main-wrapper" id="main-content">

    <div class="govuk-grid-row">
        <div class="govuk-grid-column-three-quarters">

            <h1 class="govuk-heading-xl">@ViewData["Title"]</h1>
            <p class="govuk-body">@Html.Raw(@StaticTextResource.Supplementary_PrivacyNotice_Intro)</p>
            <p class="govuk-body">@Html.Raw(@StaticTextResource.Supplementary_PrivacyNotice_ReadMore)</p>

        </div>
    </div>
</main>