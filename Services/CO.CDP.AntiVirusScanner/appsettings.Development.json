{
  "Logging": {
    "Console": true,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "CO.CDP": "Debug",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Aws": {
    "Credentials": {
      "AccessKeyId": "test",
      "SecretAccessKey": "test"
    },
    "ServiceURL": "http://localhost:4566",
    "Buckets": {
      "StagingBucket": "cdp-staging-bucket.s3",
      "PermanentBucket": "cdp-permanent-bucket.s3"
    },
    "SqsDispatcher": {
      "QueueUrl": "http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/av-scanner.fifo",
      "MaxNumberOfMessages": 10,
      "WaitTimeSeconds": 20
    },
    "SqsPublisher": {
      "QueueUrl": "http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/av-scanner.fifo"
    },
    "CloudWatch": {
      "LogGroup": "localstack",
      "LogStream": "serilog"
    }
  },
  "EntityVerificationDatabase": {
    "Server": "localhost",
    "Database": "cdp",
    "Username": "cdp_user",
    "Password": "cdp123"
  },
  "Organisation:Authority": "http://localhost:8092",
  "Features": {
    "SwaggerUI": true
  },
  "ClamAvScanUrl": "http://localhost:9000/scan"
}
