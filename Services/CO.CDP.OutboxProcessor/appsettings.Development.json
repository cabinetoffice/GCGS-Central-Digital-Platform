{
  "Logging": {
    "Console": true,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "CO.CDP": "Debug",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "Channel": "entity_verification_outbox",
  "OutboxDatabase": {
    "Server": "localhost",
    "Database": "cdp",
    "Username": "cdp_user",
    "Password": "cdp123",
    "MaxPoolSize": 10
  },
  "Aws": {
    "Credentials": {
      "AccessKeyId": "test",
      "SecretAccessKey": "test"
    },
    "ServiceURL": "http://localhost:4566",
    "SqsDispatcher": {
      "QueueUrl": "http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/av-scanner.fifo",
      "MaxNumberOfMessages": 10,
      "WaitTimeSeconds": 20
    },
    "CloudWatch": {
      "LogGroup": "localstack",
      "LogStream": "serilog"
    }
  }
}
