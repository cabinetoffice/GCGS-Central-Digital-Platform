{
  "Logging": {
    "Console": true,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "Aws": {
    "ServiceURL": "http://localhost:4566",
    "CloudWatch": {
      "LogGroup": "localstack",
      "LogStream": "serilog"
    },
    "ElastiCache": {
      "Hostname": "localhost",
      "Port": "6379"
    }
  },
  "Features": {
    "CommercialTools": true
  },
  "Redis": {
    "InstanceName": "RoctCache:",
    "DefaultExpirationMinutes": 60
  },
  "ODataApi": {
    "BaseUrl": "https://staging.odi.data-platform.codatplat.com/odata"
  }
}
