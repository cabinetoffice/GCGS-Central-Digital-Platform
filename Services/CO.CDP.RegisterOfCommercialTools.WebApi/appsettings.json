{
  "Version": "dev",
  "Logging": {
    "Console": false,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "SwaggerUI": false,
    "CommercialTools": false
  },
  "ForwardedHeaders": {
    "KnownNetworks": []
  },
  "Aws": {
    "Region": "eu-west-2",
    "CloudWatch": {
      "LogGroup": "/ecs/commercial-tools-api",
      "LogStream": "serilog"
    },
    "CognitoAuthentication": {
      "UserPoolId": "",
      "UserPoolClientId": "",
      "UserPoolClientSecret": "",
      "Domain": ""
    }
  },
  "ODataApi": {
    "BaseUrl": "http://localhost:5259/api/v1/tender",
    "ApiKey": ""
  }
}
