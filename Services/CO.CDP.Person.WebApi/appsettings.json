{
  "Logging": {
    "Console": false,
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore.Hosting": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Version": "dev",
  "Organisation:Authority": "",
  "OrganisationInformationDatabase": {
    "Server": "localhost",
    "Database": "cdp",
    "Username": "cdp_user",
    "Password": "cdp123",
    "MaxPoolSize": 300
  },
  "ForwardedHeaders": {
    "KnownNetwork": ""
  },
  "Features": {
    "SwaggerUI": false,
    "OrganisationSyncEnabled": false
  },
  "Aws": {
    "SqsPublisher": {
      "QueueUrl": "",
      "MessageGroupId": "UserManagement",
      "Outbox": {
        "BatchSize": 10,
        "ExecutionInterval": "00:00:10"
      }
    },
    "CloudWatch": {
      "LogGroup": "/ecs/person",
      "LogStream": "serilog"
    }
  }
}
